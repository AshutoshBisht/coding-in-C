#include<stdio.h>
int main()
{
  int a[3],b[3];
  for(int i=0;i<3;i++)
  {
    scanf("%d",&a[i] );
  }
  for(int j=0;j<3;j++)
  {
    scanf("%d",&b[j]);
  }
  int a1=0,b1=0;
  for(int k=0;k<=2;k++)
  {
    if(a[k]>b[k])
  a1=a1+1;
  if(b[k]>a[k])
  b1=b1+1;
    }
    printf("%d %d",a1,b1 );
    return 0;
  }
